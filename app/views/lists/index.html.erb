<p id="notice"><%= notice %></p>

<h1>Lists</h1>

<div>
  <% @lists.each do |list| %>
    <tr>
      <h1><td><%= list.title %> </td></h1>
      <h2><td><%= list.created_at %></td></h2>
      <h3><td><%= link_to 'Show', list_path(list.id) %></td></h3>
      <h4><%= link_to 'Edit', edit_list_path(list) %></h4>
      <h5><td><%= link_to 'Destroy', list_path(list.id), method: :delete, data: { confirm: 'Are you sure?' } %></td></h5>
    </tr>
  <% end %>
</div>

  
</table>

<br>

<%= link_to 'New List', new_list_path %>
